<Window x:Class="ParetoCalculatorUI.SelectionDialog"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ParetoCalculatorUI"
        mc:Ignorable="d"
        Title="SelectionDialog" Height="450" Width="800">
    <Grid>
        <Button x:Name="okButton" Content="OK" HorizontalAlignment="Left" Height="49.404" Margin="240.869,332.785,0,0" VerticalAlignment="Top" Width="310.382" Click="okButton_Click"/>
        <Button x:Name="cancelButton" Content="Cancel" HorizontalAlignment="Left" Height="49.404" Margin="578.1,332.785,0,0" VerticalAlignment="Top" Width="161.098" Click="cancelButton_Click"/>
        <ListBox x:Name="selectionListbox" HorizontalAlignment="Left" Height="254.535" Margin="62.587,35.291,0,0" VerticalAlignment="Top" Width="637.948" MouseDoubleClick="selectionListbox_MouseDoubleClick"/>

    </Grid>
</Window>
